<mvc:View
    controllerName="zhrmsportal.controller.AddEmployee"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
>
    <NavContainer id="NavContainer">
        <!-- <App> -->
        <pages>
            <Page>
                <content>
                    <Wizard
                        id="idWizard"
                        renderMode="Page"
                        complete="onComplete"
                        backgroundDesign="Solid"
                        class="sapUiResponsivePadding--header sapUiResponsivePadding--content wizard"
                        enableBranching="false"
                    >
                        <WizardStep
                            id="PersonalDetails"
                            title="Personal Details"
                            validated="false"
                            activate="personalDetails"
                            icon="sap-icon://account"
                            class="wizard"
                        >
                            <form:Form editable="true">
                                <form:layout>
                                    <form:ResponsiveGridLayout
                                        class="wizard-form"
                                        labelSpanXL="4"
                                        labelSpanL="4"
                                        labelSpanM="4"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                        emptySpanXL="4"
                                        emptySpanL="4"
                                        emptySpanM="0"
                                        emptySpanS="0"
                                        columnsXL="6"
                                        columnsL="2"
                                        columnsM="2"
                                    />
                                </form:layout>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Name">
                                            <form:fields>
                                                <!-- id="fName" -->
                                                <Input
                                                    value="{FormData>/EmployeeData/fn}"
                                                    required="true"
                                                    liveChange="personalDetails"
                                                    placeholder="First Name"
                                                />
                                                <!-- id="lName" -->
                                                <Input
                                                    value="{FormData>/EmployeeData/ln}"
                                                    required="true"
                                                    liveChange="personalDetails"
                                                    placeholder="Last Name"
                                                />
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement label="Email">
                                            <!-- id="pEmail" -->
                                            <form:fields>
                                                <Input
                                                    required="true"
                                                    value="{FormData>/EmployeeData/EMP_EMAIL}"
                                                    liveChange="personalDetails"
                                                    placeholder="Email"
                                                />
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement label="Mobile">
                                            <form:fields>
                                                <Input
                                                    value="{FormData>/EmployeeData/EMP_MOBILE}"
                                                    required="true"
                                                    type="Number"
                                                    liveChange="personalDetails"
                                                    placeholder="Mobile Number"
                                                />
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement label="Date of Birth">
                                            <form:fields>
                                                <DatePicker
                                                    displayFormat="dd.MM.yyyy"
                                                    valueFormat="yyyy-MM-dd"
                                                    value="{FormData>/EmployeeData/EMP_DOB}"
                                                />
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                                <!-- <Input/> -->
                            </form:Form>
                        </WizardStep>
                        <WizardStep
                            id="JobDetails"
                            title="Job Details"
                            validated="false"
                            activate="jobDetails"
                            icon="sap-icon://employee"
                            class="wizard"
                        >
                            <form:Form
                                editable="true"
                                layout="ResponsiveGridLayout"
                            >
                                <form:layout>
                                    <form:ResponsiveGridLayout
                                        labelSpanXL="4"
                                        labelSpanL="4"
                                        labelSpanM="4"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                        emptySpanXL="0"
                                        emptySpanL="0"
                                        emptySpanM="0"
                                        emptySpanS="0"
                                        columnsXL="6"
                                        columnsL="2"
                                        columnsM="2"
                                        singleContainerFullSize="false"
                                    />
                                </form:layout>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Employee Type">
                                            <form:fields>
                                                <RadioButtonGroup
                                                    id="empType"
                                                    class="radio"
                                                    selectedIndex="{FormData>/EmployeeData/selType}"
                                                    buttons="{FormData>/EmoloyeeType}"
                                                >
                                                    <RadioButton
                                                        text="{FormData>empTName}"
                                                        class="radioButtons"
                                                    />
                                                </RadioButtonGroup>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement label="Employee Code">
                                            <form:fields>
                                                <Input
                                                    required="true"
                                                    value="{FormData>/EmployeeData/EMP_CODE}"
                                                    liveChange="jobDetails"
                                                    placeholder="Employee Code"
                                                />
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement label="Module">
                                            <ComboBox
                                            selectionChange="jobDetails"
                                                required="true"
                                                selectedKey="{FormData>/EmployeeData/selModule}"
                                                items="{Modules>/}"
                                                placeholder="Select Module"
                                            >
                                                <core:Item
                                                    key="{Modules>MODULE_ID}"
                                                    text="{Modules>MODULE_NAME}"
                                                />
                                            </ComboBox>
                                            <!-- <ComboBox
                                                
                                                    items="{Modules>/Modules}"
                                                    selectionChange="onModuleSelect"
                                                    placeholder="Select Module Type"
                                                    required="true"
                                                    selectedKey="{Modules>/selMT}"
                                                >
                                                    <core:Item
                                                        key="{Modules>mKey}"
                                                        text="{Modules>mName}"
                                                    />
                                                </ComboBox> -->
                                            <!-- items="{Modules>/selM}" -->
                                            <!-- <ComboBox
                                                    visible="false"
                                                    id="modules"
                                                    placeholder="Select module"
                                                    selectionChange="onModuleSelect"
                                                    selectedItem="{Modules>/selM}"
                                                > -->
                                            <!-- <core:Item
                                                        key="{Modules>mKey}"
                                                        text="{Modules>mName}"
                                                    /> -->
                                            <!-- </ComboBox>  -->
                                        </form:FormElement>
                                        <form:FormElement label="Employee Role">
                                            <form:fields>
                                                <ComboBox
                                                selectionChange="jobDetails"
                                                    items="{FormData>/EmployeeRole}"
                                                    selectedKey="{FormData>/EmployeeData/selRole}"
                                                    placeholder="Select Employee Role"
                                                    required="true"
                                                >
                                                    <core:Item
                                                        key="{FormData>empRKey}"
                                                        text="{FormData>empRName}"
                                                    />
                                                </ComboBox>
                                            </form:fields>
                                        </form:FormElement>
                                        <!-- <form:FormElement
                                            label="Official Email"
                                        >
                                            <form:fields>
                                                <Input
                                                    required="true"
                                                    value="{FormData>/EmployeeData/ofcEmail}"
                                                    placeholder="Official email"
                                                />
                                            </form:fields>
                                        </form:FormElement> -->
                                        <form:FormElement label="Status">
                                            <form:fields>
                                                <ComboBox
                                                selectionChange="jobDetails"
                                                    required="true"
                                                    items="{FormData>/Status}"
                                                    selectedKey="{FormData>/EmployeeData/selStatus}"
                                                    placeholder="Status"
                                                >
                                                    <core:Item
                                                        key="{FormData>sKey}"
                                                        text="{FormData>sValue}"
                                                    />
                                                </ComboBox>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                    <form:layoutData>
                                        <form:GridContainerData
                                            halfGrid="true"
                                        />
                                    </form:layoutData>
                                </form:FormContainer>

                                <form:FormContainer>
                                <form:FormElement
                                            label="Date of Joining"
                                        >
                                            <form:fields>
                                                <DatePicker
                                                    required="true"
                                                    displayFormat="dd.MM.yyyy"
                                                    valueFormat="yyyy-MM-dd"
                                                    value="{FormData>/EmployeeData/DATE_OF_JOINING}"
                                                />
                                            </form:fields>
                                        </form:FormElement>
                                    <form:formElements>
                                        <form:FormElement label="Designation">
                                            <form:fields>
                                                <Input
                                                    required="true"
                                                    value="{FormData>/EmployeeData/DESIGNATION}"
                                                    liveChange="jobDetails"
                                                    placeholder="Designation"
                                                />
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement
                                            label="Reporting Manager"
                                        >
                                            <form:fields>
                                                <MultiComboBox
                                                    required="true"
                                                    placeholder="Select Manager"
                                                    selectionChange="jobDetails"
                                                    selectionFinish="handleSelectionFinish"
                                                    selectedKeys="{FormData>/EmployeeData/selManager}"
                                                    items="{FormData>/ReportingManager}"
                                                >
                                                    <core:Item
                                                        key="{FormData>repoMKey}"
                                                        text="{FormData>repoMName}"
                                                    />
                                                </MultiComboBox>
                                            </form:fields>
                                        </form:FormElement>
                                        
                                        <!-- <form:FormElement label="Punch Id">
                                            <form:fields>
                                                <Input
                                                    value="{FormData>/EmployeeData/punchID}"
                                                    liveChange="personalDetails"
                                                    placeholder="Punch Id"
                                                />
                                            </form:fields>
                                        </form:FormElement> -->
                                    </form:formElements>
                                    <form:layoutData>
                                        <form:GridContainerData
                                            halfGrid="true"
                                        />
                                    </form:layoutData>
                                </form:FormContainer>
                            </form:Form>
                        </WizardStep>

                        <WizardStep
                            id="EducationDetails"
                            title="Education Details"
                            validated="true"
                            activate="educationDetails"
                            icon="sap-icon://study-leave"
                        >
                            <form:Form editable="true">
                                <form:layout>
                                    <form:ResponsiveGridLayout
                                        labelSpanXL="4"
                                        labelSpanL="4"
                                        labelSpanM="4"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                        emptySpanXL="4"
                                        emptySpanL="4"
                                        emptySpanM="0"
                                        emptySpanS="0"
                                        columnsXL="6"
                                        columnsL="2"
                                        columnsM="2"
                                    />
                                </form:layout>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Degree Type">
                                            <form:fields>
                                                <Input
                                                    placeholder="Degree Type"
                                                    value="{FormData>/EmployeeData/degType}"
                                                    liveChange="educationDetails"
                                                    required="true"
                                                />
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement
                                            label="Graduation Date"
                                        >
                                            <DatePicker
                                                placeholder="Select Graduation Date"
                                                displayFormat="dd.MM.yyyy"
                                                valueFormat="yyyy-MM-dd"
                                                value="{FormData>/EmployeeData/GRADUATION_DATE}"
                                            />
                                        </form:FormElement>
                                        <form:FormElement
                                            label="Insititute Name"
                                        >
                                            <Input
                                                placeholder="Enter Insititute Name"
                                                value="{FormData>/EmployeeData/INSTITUTION_NAME}"
                                                liveChange="educationDetails"
                                                required="true"
                                            />
                                        </form:FormElement>
                                        <form:FormElement label="CGPA">
                                            <Input
                                                placeholder="Enter CGPA"
                                                value="{FormData>/EmployeeData/CGPA}"
                                                liveChange="educationDetails"
                                                required="true"
                                            />
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                            </form:Form>
                        </WizardStep>
                        <WizardStep
                            id="SignatureStep"
                            title="Signature"
                            validated="true"
                            activate="signature"
                            icon="sap-icon://signature"
                        >
                            <VBox>
                                <core:HTML id="html" />
                            </VBox>
                            <VBox>
                                <HBox>
                                    <Button
                                        id="Signature"
                                        text="Signature"
                                        press="onSign"
                                    />
                                    <Button
                                        id="Save"
                                        text="Save"
                                        press="saveButton"
                                    />
                                    <Button
                                        id="clear"
                                        text="Clear"
                                        press="clearButton"
                                    />
                                </HBox>
                            </VBox>

